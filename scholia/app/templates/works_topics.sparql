#defaultView:BubbleChart
SELECT
  ?count
  ?topic ?topicLabel
WITH {
  SELECT
    (COUNT(?work) AS ?count)
    ?topic 
  WHERE {
    VALUES ?work {  {% for q in qs %} wd:{{ q }} {% endfor %}  }
    ?work wdt:P921 ?topic .
  }
  GROUP BY ?topic
  ORDER BY ?count  
  LIMIT 200
} AS %results
WHERE {
  INCLUDE %results
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language "[AUTO_LANGUAGE],en". }
  }
ORDER BY DESC(?count)
